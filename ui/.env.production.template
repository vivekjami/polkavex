# Production Environment Configuration for UI
# Copy this file to .env.production and update the values

# API Configuration
REACT_APP_API_URL=https://your-relayer-domain.com
REACT_APP_WS_URL=wss://your-relayer-domain.com

# Environment
REACT_APP_ENVIRONMENT=production
REACT_APP_VERSION=1.0.0

# Blockchain Configuration
REACT_APP_CHAIN_ID=1
REACT_APP_NETWORK=mainnet
REACT_APP_ETHEREUM_RPC=https://mainnet.infura.io/v3/YOUR_INFURA_KEY

# Analytics (Optional)
REACT_APP_ANALYTICS_ID=your_google_analytics_id
REACT_APP_SENTRY_DSN=your_sentry_dsn

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_DEBUG_MODE=false
